{
	"name": "vrc-hub",
	"version": "0.0.1",
	"author": "Luna Simons <luna@bddvlpr.com> (https://bddvlpr.com)",
	"license": "BSD-3-Clause",
	"packageManager": "pnpm@^8.8.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . && eslint . --fix",
		"struct": "openapi-typescript https://vrchatapi.github.io/specification/openapi.yaml --output src/lib/api/vrchat/spec.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/typography": "^0.5.10",
		"@tauri-apps/api": "^1.5.1",
		"@tauri-apps/cli": "^1.5.6",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.4.6",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lucide-svelte": "^0.287.0",
		"openapi-typescript": "^6.7.1",
		"openapi-typescript-helpers": "^0.0.4",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.1",
		"svelte-persisted-store": "^0.7.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0"
	},
	"type": "module"
}
